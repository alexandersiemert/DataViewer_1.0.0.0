<Window x:Class="DataViewer_1._0._0._0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataViewer_1._0._0._0"
        xmlns:ScottPlot="clr-namespace:ScottPlot.WPF;assembly=ScottPlot.WPF"
        xmlns:fa="http://schemas.fontawesome.io/icons/"
        mc:Ignorable="d"
        Title="SIEMERT DataViewer 1.0.0" Height="600" Width="1050" MinHeight="450" MinWidth="1050" Icon="/Icons/Icon.png" Closed="Window_Closed">


    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition Height="20"/>
            <RowDefinition Height="30"/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <DockPanel Background="White">
            <Menu DockPanel.Dock="Top">

                <MenuItem Header="File">
                    <MenuItem Header="Open" Click="menuItemOpen_Click">
                        <MenuItem.Icon>
                            <fa:FontAwesome Icon="FolderOpen" VerticalAlignment="Center"/>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="Save" Click="menuItemSave_Click">
                        <MenuItem.Icon>
                            <fa:FontAwesome Icon="Save" VerticalAlignment="Center"/>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="Save All" Click="menuItemSaveAll_Click">
                        <MenuItem.Icon>
                            <fa:FontAwesome Icon="Save" VerticalAlignment="Center"/>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="Export csv" Click="menuItemExportCsv_Click">
                        <MenuItem.Icon>
                            <fa:FontAwesome Icon="Share" VerticalAlignment="Center"/>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="Close">
                        <MenuItem.Icon>
                            <fa:FontAwesome Icon="Close" VerticalAlignment="Center"/>
                        </MenuItem.Icon>
                    </MenuItem>
                </MenuItem>

                <MenuItem Header="Settings">
                    <MenuItem Header="Chart Style">
                        <MenuItem.Icon>
                            <fa:FontAwesome Icon="LineChart" VerticalAlignment="Center"/>
                        </MenuItem.Icon>
                        <MenuItem x:Name="menuItemChartStyleLight" Header="Light" IsCheckable="True" IsChecked="True" Tag="Light" Click="menuItemChartStyle_Click"/>
                        <MenuItem x:Name="menuItemChartStyleDark" Header="Dark" IsCheckable="True" Tag="Dark" Click="menuItemChartStyle_Click"/>
                        <MenuItem x:Name="menuItemChartStyleSlate" Header="Slate" IsCheckable="True" Tag="Slate" Click="menuItemChartStyle_Click"/>
                        <Separator/>
                        <MenuItem Header="Background">
                            <MenuItem Header="Figure..." Tag="FigureBackground" Click="menuItemChartStyleColor_Click"/>
                            <MenuItem Header="Plot..." Tag="DataBackground" Click="menuItemChartStyleColor_Click"/>
                        </MenuItem>
                        <MenuItem Header="Grid">
                            <MenuItem x:Name="menuItemChartStyleShowGrid" Header="Show Grid" IsCheckable="True" IsChecked="True" Click="menuItemChartStyleGrid_Click"/>
                            <MenuItem Header="Grid Color..." Tag="GridMajor" Click="menuItemChartStyleColor_Click"/>
                        </MenuItem>
                        <MenuItem Header="Axes">
                            <MenuItem Header="Axis Color..." Tag="AxisColor" Click="menuItemChartStyleColor_Click"/>
                            <MenuItem x:Name="menuItemAxisLabelFollowSeries" Header="Axis Labels Follow Series" IsCheckable="True" IsChecked="True" Click="menuItemAxisLabelMode_Click"/>
                            <MenuItem Header="Axis Label Colors">
                                <MenuItem x:Name="menuItemAxisLabelAltColor" Header="Altitude..." Tag="AxisLabelAlt" Click="menuItemChartStyleColor_Click"/>
                                <MenuItem x:Name="menuItemAxisLabelTempColor" Header="Temperature..." Tag="AxisLabelTemp" Click="menuItemChartStyleColor_Click"/>
                                <MenuItem x:Name="menuItemAxisLabelAccColor" Header="Acceleration..." Tag="AxisLabelAcc" Click="menuItemChartStyleColor_Click"/>
                            </MenuItem>
                        </MenuItem>
                        <MenuItem Header="Legend">
                            <MenuItem Header="Background..." Tag="LegendBackground" Click="menuItemChartStyleColor_Click"/>
                            <MenuItem Header="Text..." Tag="LegendText" Click="menuItemChartStyleColor_Click"/>
                            <MenuItem Header="Outline..." Tag="LegendOutline" Click="menuItemChartStyleColor_Click"/>
                        </MenuItem>
                        <MenuItem Header="Series Colors">
                            <MenuItem Header="Altitude..." Tag="SeriesAlt" Click="menuItemChartStyleColor_Click"/>
                            <MenuItem Header="Temperature..." Tag="SeriesTemp" Click="menuItemChartStyleColor_Click"/>
                            <MenuItem Header="Acc Abs..." Tag="SeriesAccAbs" Click="menuItemChartStyleColor_Click"/>
                            <MenuItem Header="Acc X..." Tag="SeriesAccX" Click="menuItemChartStyleColor_Click"/>
                            <MenuItem Header="Acc Y..." Tag="SeriesAccY" Click="menuItemChartStyleColor_Click"/>
                            <MenuItem Header="Acc Z..." Tag="SeriesAccZ" Click="menuItemChartStyleColor_Click"/>
                        </MenuItem>
                        <MenuItem Header="Series Line Width">
                            <MenuItem Header="Altitude..." Tag="LineWidthAlt" Click="menuItemSeriesLineWidth_Click"/>
                            <MenuItem Header="Temperature..." Tag="LineWidthTemp" Click="menuItemSeriesLineWidth_Click"/>
                            <MenuItem Header="Acc Abs..." Tag="LineWidthAccAbs" Click="menuItemSeriesLineWidth_Click"/>
                            <MenuItem Header="Acc X..." Tag="LineWidthAccX" Click="menuItemSeriesLineWidth_Click"/>
                            <MenuItem Header="Acc Y..." Tag="LineWidthAccY" Click="menuItemSeriesLineWidth_Click"/>
                            <MenuItem Header="Acc Z..." Tag="LineWidthAccZ" Click="menuItemSeriesLineWidth_Click"/>
                        </MenuItem>
                        <MenuItem Header="Series Line Pattern">
                            <MenuItem Header="Altitude">
                                <MenuItem Header="Solid" Tag="LinePatternAlt|Solid" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Dashed" Tag="LinePatternAlt|Dashed" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Densely Dashed" Tag="LinePatternAlt|DenselyDashed" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Dotted" Tag="LinePatternAlt|Dotted" Click="menuItemSeriesLinePattern_Click"/>
                            </MenuItem>
                            <MenuItem Header="Temperature">
                                <MenuItem Header="Solid" Tag="LinePatternTemp|Solid" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Dashed" Tag="LinePatternTemp|Dashed" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Densely Dashed" Tag="LinePatternTemp|DenselyDashed" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Dotted" Tag="LinePatternTemp|Dotted" Click="menuItemSeriesLinePattern_Click"/>
                            </MenuItem>
                            <MenuItem Header="Acc Abs">
                                <MenuItem Header="Solid" Tag="LinePatternAccAbs|Solid" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Dashed" Tag="LinePatternAccAbs|Dashed" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Densely Dashed" Tag="LinePatternAccAbs|DenselyDashed" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Dotted" Tag="LinePatternAccAbs|Dotted" Click="menuItemSeriesLinePattern_Click"/>
                            </MenuItem>
                            <MenuItem Header="Acc X">
                                <MenuItem Header="Solid" Tag="LinePatternAccX|Solid" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Dashed" Tag="LinePatternAccX|Dashed" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Densely Dashed" Tag="LinePatternAccX|DenselyDashed" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Dotted" Tag="LinePatternAccX|Dotted" Click="menuItemSeriesLinePattern_Click"/>
                            </MenuItem>
                            <MenuItem Header="Acc Y">
                                <MenuItem Header="Solid" Tag="LinePatternAccY|Solid" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Dashed" Tag="LinePatternAccY|Dashed" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Densely Dashed" Tag="LinePatternAccY|DenselyDashed" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Dotted" Tag="LinePatternAccY|Dotted" Click="menuItemSeriesLinePattern_Click"/>
                            </MenuItem>
                            <MenuItem Header="Acc Z">
                                <MenuItem Header="Solid" Tag="LinePatternAccZ|Solid" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Dashed" Tag="LinePatternAccZ|Dashed" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Densely Dashed" Tag="LinePatternAccZ|DenselyDashed" Click="menuItemSeriesLinePattern_Click"/>
                                <MenuItem Header="Dotted" Tag="LinePatternAccZ|Dotted" Click="menuItemSeriesLinePattern_Click"/>
                            </MenuItem>
                        </MenuItem>
                        <Separator/>
                        <MenuItem Header="Reset Style" Click="menuItemChartStyleReset_Click"/>
                        <MenuItem Header="Reset Series Colors" Click="menuItemSeriesColorsReset_Click"/>
                        <MenuItem Header="Reset Line Styles" Click="menuItemSeriesLineStylesReset_Click"/>
                    </MenuItem>
                    <MenuItem Header="Units">
                        <MenuItem.Icon>
                            <fa:FontAwesome Icon="Calculator" VerticalAlignment="Center"/>
                        </MenuItem.Icon>
                        <MenuItem x:Name="menuItemUnitsMetric" Header="Metric (m, &#176;C)" IsCheckable="True" Click="menuItemUnitsMetric_Click"/>
                        <MenuItem x:Name="menuItemUnitsImperial" Header="Imperial (ft, &#176;F)" IsCheckable="True" Click="menuItemUnitsImperial_Click"/>
                    </MenuItem>
                    <MenuItem Header="Series">
                        <MenuItem x:Name="menuItemSeriesAlt" Header="Altitude" IsCheckable="True" IsChecked="True" Tag="Alt" Click="menuItemSeries_Click"/>
                        <MenuItem x:Name="menuItemSeriesTemp" Header="Temperature" IsCheckable="True" IsChecked="True" Tag="Temp" Click="menuItemSeries_Click"/>
                        <MenuItem x:Name="menuItemSeriesAccAbs" Header="Acc Abs" IsCheckable="True" IsChecked="True" Tag="AccAbs" Click="menuItemSeries_Click"/>
                        <MenuItem x:Name="menuItemSeriesAccX" Header="Acc X" IsCheckable="True" IsChecked="True" Tag="AccX" Click="menuItemSeries_Click"/>
                        <MenuItem x:Name="menuItemSeriesAccY" Header="Acc Y" IsCheckable="True" IsChecked="True" Tag="AccY" Click="menuItemSeries_Click"/>
                        <MenuItem x:Name="menuItemSeriesAccZ" Header="Acc Z" IsCheckable="True" IsChecked="True" Tag="AccZ" Click="menuItemSeries_Click"/>
                    </MenuItem>
                </MenuItem>
                <MenuItem Header="Help">
                    <MenuItem Header="Docs" Click="menuItemHelpDocs_Click">
                        <MenuItem.Icon>
                            <fa:FontAwesome Icon="Book" VerticalAlignment="Center"/>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="About" Click="menuItemHelpAbout_Click">
                        <MenuItem.Icon>
                            <fa:FontAwesome Icon="InfoCircle" VerticalAlignment="Center"/>
                        </MenuItem.Icon>
                    </MenuItem>

                </MenuItem>
            </Menu>
        </DockPanel>

        <Border Grid.Row="1" Background="#38383D" BorderBrush="#272539" BorderThickness="0.2">
            <Grid>
                <Image Source="/Pictures/dataviewer.png" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="8,8,8,8"/>


                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="5,5,5,5">

                    <ToggleButton x:Name="toggleButtonMeasuringCursor" Content="Measuring Cursor" Width="120" VerticalAlignment="Center" Checked="toggleButtonMeasuringCursor_Checked" Unchecked="toggleButtonMeasuringCursor_Unchecked">
                        <ToggleButton.Resources>
                            <Style TargetType="Border">
                                <Setter Property="CornerRadius" Value="5"/>
                            </Style>
                        </ToggleButton.Resources>
                    </ToggleButton>

                    <ToggleButton x:Name="toggleButtonCrosshair" Content="Crosshair" Width="70" Margin="5,0,0,0" Checked="toggleButtonCrosshair_Checked" Unchecked="toggleButtonCrosshair_Unchecked">
                        <ToggleButton.Resources>
                            <Style TargetType="Border">
                                <Setter Property="CornerRadius" Value="5"/>
                            </Style>
                        </ToggleButton.Resources>
                    </ToggleButton>

                    <ToggleButton x:Name="toggleButtonMarker" Content="Marker" Width="60" Margin="5,0,0,0" Checked="toggleButtonMarker_Checked" Unchecked="toggleButtonMarker_Unchecked">
                        <ToggleButton.Resources>
                            <Style TargetType="Border">
                                <Setter Property="CornerRadius" Value="5"/>
                            </Style>
                        </ToggleButton.Resources>
                    </ToggleButton>

                    <ToggleButton x:Name="toggleButtonLegend" Content="Legend" Width="60" Margin="5,0,0,0" Checked="toggleButtonLegend_Checked" Unchecked="toggleButtonLegend_Unchecked">
                        <ToggleButton.Resources>
                            <Style TargetType="Border">
                                <Setter Property="CornerRadius" Value="5"/>
                            </Style>
                        </ToggleButton.Resources>
                    </ToggleButton>

                </StackPanel>
                <Image Source="/Pictures/siemert.png" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="8,8,8,8"/>
            </Grid>
        </Border>


        <Grid Grid.Row="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition MinWidth="150" MaxWidth="200" Width="150"/>
                <ColumnDefinition Width="3"/>
                <ColumnDefinition/>
                <ColumnDefinition Width="170"/>
            </Grid.ColumnDefinitions>

            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="20"/>
                    <RowDefinition/>
                    <RowDefinition Height="20"/>
                    <RowDefinition Height="250"/>
                </Grid.RowDefinitions>

                <Border BorderThickness="0.2" BorderBrush="#272539">
                    <Grid Background="#FFCCD8E6">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>

                        <!-- Element links ausrichten -->
                        <StackPanel Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Stretch">
                            <fa:ImageAwesome Icon="CodeFork" Margin="5,3,0,3" Rotation="90" HorizontalAlignment="Left"/>
                            <TextBlock Text="Device List" Margin="8,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                        </StackPanel>

                        <!-- Element ganz rechts ausrichten -->
                        <Button x:Name="buttonRefreshDeviceList" Grid.Column="2" Content="Refresh" Click="buttonRefreshDeviceList_Click" HorizontalAlignment="Right" Margin="0,0,5,0" Height="18" Width="50">
                            <Button.Resources>
                                <Style TargetType="Border">
                                    <Setter Property="CornerRadius" Value="5"/>
                                </Style>
                            </Button.Resources>
                        </Button>
                    </Grid>
                </Border>

                <TreeView x:Name="deviceListTreeView" Grid.Row="1">
                </TreeView>

                <Border BorderBrush="Black" BorderThickness="0.4" Background="#FFCCD8E6" VerticalAlignment="Top" Height="20" Grid.Row="2">
                    <StackPanel Orientation="Horizontal">
                        <fa:ImageAwesome Icon="InfoCircle" Margin="5,3,0,3"/>
                        <TextBlock Text="Device Info" Margin="8,0,0,0" VerticalAlignment="Center"/>
                    </StackPanel>
                </Border>


                <StackPanel Grid.Row="3" Orientation="Vertical">
                    <StackPanel Orientation="Vertical">
                        <TextBlock Text="Model" Width="Auto" FontWeight="Bold" HorizontalAlignment="Left"/>
                        <TextBox x:Name="textBoxModel" Text="-" VerticalAlignment="Center" IsReadOnly="True" BorderBrush="Transparent"/>
                    </StackPanel>
                    <StackPanel Orientation="Vertical" HorizontalAlignment="Left">
                        <TextBlock Text="Serial No." Width="Auto" FontWeight="Bold"/>
                        <TextBox x:Name="textBoxSerialNumber" Text="-" VerticalAlignment="Center" IsReadOnly="True" BorderBrush="Transparent"/>
                    </StackPanel>
                    <StackPanel Orientation="Vertical" HorizontalAlignment="Left">
                        <TextBlock Text="Date of production" Width="Auto" FontWeight="Bold"/>
                        <TextBox x:Name="textBoxProductionDate" Text="-" VerticalAlignment="Center" IsReadOnly="True" BorderBrush="Transparent"/>
                    </StackPanel>
                    <StackPanel Orientation="Vertical" HorizontalAlignment="Left">
                        <TextBlock Text="Checksum" Width="Auto" FontWeight="Bold"/>
                        <TextBox x:Name="textBoxChecksum" Text="-" VerticalAlignment="Center" IsReadOnly="True" BorderBrush="Transparent"/>
                    </StackPanel>
                </StackPanel>

            </Grid>

            <GridSplitter Grid.Column="1" ResizeDirection="Columns" HorizontalAlignment="Stretch"/>

            <Grid Grid.Column="2">
                <Grid.RowDefinitions>
                    <RowDefinition Height="20"/>
                    <RowDefinition/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>

                <Border BorderBrush="Black" BorderThickness="0.4" Background="#FFCCD8E6">
                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                        <fa:ImageAwesome Icon="Crosshairs" Margin="5,3,0,3" Rotation="90"/>
                        <TextBlock Text="Crosshair" Margin="8,0,0,0" VerticalAlignment="Center"/>
                        <TextBlock Text="Alt" Margin="8,0,0,0" VerticalAlignment="Center" FontWeight="Bold"/>
                        <TextBox x:Name="textBoxCrossAlt" Text="NaN" Margin="8,0,0,0" VerticalAlignment="Center" IsReadOnly="True" Height="18"/>
                        <TextBlock Text="Temp" Margin="8,0,0,0" VerticalAlignment="Center" FontWeight="Bold"/>
                        <TextBox x:Name="textBoxCrossTemp" Text="NaN" Margin="8,0,0,0" VerticalAlignment="Center" IsReadOnly="True" Height="18"/>
                        <TextBlock Text="Acc" Margin="8,0,0,0" VerticalAlignment="Center" FontWeight="Bold"/>
                        <TextBox x:Name="textBoxCrossAcc" Text="NaN" Margin="8,0,0,0" VerticalAlignment="Center" IsReadOnly="True" Height="18"/>
                        <TextBlock Text="Acc-X" Margin="8,0,0,0" VerticalAlignment="Center" FontWeight="Bold"/>
                        <TextBox x:Name="textBoxCrossAccX" Text="NaN" Margin="8,0,0,0" VerticalAlignment="Center" IsReadOnly="True" Height="18"/>
                        <TextBlock Text="Acc-Y" Margin="8,0,0,0" VerticalAlignment="Center" FontWeight="Bold"/>
                        <TextBox x:Name="textBoxCrossAccY" Text="NaN" Margin="8,0,0,0" VerticalAlignment="Center" IsReadOnly="True" Height="18"/>
                        <TextBlock Text="Acc-Z" Margin="8,0,0,0" VerticalAlignment="Center" FontWeight="Bold"/>
                        <TextBox x:Name="textBoxCrossAccZ" Text="NaN" Margin="8,0,0,0" VerticalAlignment="Center" IsReadOnly="True" Height="18"/>
                        
                    </StackPanel>
                </Border>

                <ScottPlot:WpfPlot Name="WpfPlot1" Grid.Row="1"/>

                <Border Grid.Row="2" BorderBrush="Transparent" BorderThickness="0.4">
                    <WrapPanel Orientation="Horizontal" VerticalAlignment="Center">
                        <GroupBox Height="Auto" Header="Altitude Axis">
                            <StackPanel Orientation="Horizontal">
                                <StackPanel Orientation="Vertical">
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                        <TextBlock Text="max" Width="25" Margin="5,0,5,0" FontWeight="Bold"/>
                                        <TextBox x:Name="textBoxAltMax" KeyDown="textBoxAltMax_KeyDown" AcceptsReturn="False" Width="60" HorizontalContentAlignment="Right"/>
                                        <TextBlock x:Name="textBlockAltAxisMaxUnit" Text="m" Margin="5,0,0,0" FontWeight="Bold"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                        <TextBlock Text="min" Width="25" Margin="5,0,5,0" FontWeight="Bold"/>
                                        <TextBox x:Name="textBoxAltMin" KeyDown="textBoxAltMin_KeyDown" AcceptsReturn="False" Width="60" HorizontalContentAlignment="Right"/>
                                        <TextBlock x:Name="textBlockAltAxisMinUnit" Text="m" Margin="5,0,0,0" FontWeight="Bold"/>
                                    </StackPanel>
                                </StackPanel>
                                <StackPanel Orientation="Vertical" VerticalAlignment="Center" Margin="10,0,0,0">
                                    <Button x:Name="buttonLimitAltUp" Width="20" Height="15" PreviewMouseDown="buttonLimitAltUp_PreviewMouseDown" PreviewMouseUp="buttonLimitAltUp_PreviewMouseUp">
                                        <Button.Background>
                                            <ImageBrush ImageSource="/Pictures/up-arrow.png" Stretch="Uniform"/>
                                        </Button.Background>
                                    </Button>
                                    <Button x:Name="buttonLimitAltDown" Width="20" Height="15" RenderTransformOrigin="0.5,0.5" PreviewMouseDown="buttonLimitAltDown_PreviewMouseDown" PreviewMouseUp="buttonLimitAltDown_PreviewMouseUp">
                                        <Button.RenderTransform>
                                            <TransformGroup>
                                                <ScaleTransform ScaleY="-1"/>
                                                
                                            </TransformGroup>
                                        </Button.RenderTransform>
                                        <Button.Background>
                                            <ImageBrush ImageSource="/Pictures/up-arrow.png" Stretch="Uniform" TileMode="None"/>
                                        </Button.Background>
                                    </Button>
                                </StackPanel>
                            </StackPanel>
                        </GroupBox>
                        <GroupBox Height="Auto" Header="Temperature Axis">
                            <StackPanel Orientation="Horizontal">
                                <StackPanel Orientation="Vertical">
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                        <TextBlock Text="max" Width="25" Margin="5,0,5,0" FontWeight="Bold"/>
                                        <TextBox x:Name="textBoxTempMax" KeyDown="textBoxTempMax_KeyDown" AcceptsReturn="False" Width="60" HorizontalContentAlignment="Right"/>
                                        <TextBlock x:Name="textBlockTempAxisMaxUnit" Text="&#176;C" Margin="5,0,0,0" FontWeight="Bold"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                        <TextBlock Text="min" Width="25" Margin="5,0,5,0" FontWeight="Bold"/>
                                        <TextBox x:Name="textBoxTempMin" KeyDown="textBoxTempMin_KeyDown" AcceptsReturn="False" Width="60" HorizontalContentAlignment="Right"/>
                                        <TextBlock x:Name="textBlockTempAxisMinUnit" Text="&#176;C" Margin="5,0,0,0" FontWeight="Bold"/>
                                    </StackPanel>
                                </StackPanel>
                                <StackPanel Orientation="Vertical" VerticalAlignment="Center" Margin="10,0,0,0">
                                    <Button x:Name="buttonLimitTempUp" Width="20" Height="15" PreviewMouseDown="buttonLimitTempUp_PreviewMouseDown" PreviewMouseUp="buttonLimitTempUp_PreviewMouseUp">
                                        <Button.Background>
                                            <ImageBrush ImageSource="/Pictures/up-arrow.png" Stretch="Uniform"/>
                                        </Button.Background>
                                    </Button>
                                    <Button x:Name="buttonLimitTempDown" Width="20" Height="15" RenderTransformOrigin="0.5,0.5" PreviewMouseDown="buttonLimitTempDown_PreviewMouseDown" PreviewMouseUp="buttonLimitTempDown_PreviewMouseUp">
                                        <Button.RenderTransform>
                                            <TransformGroup>
                                                <ScaleTransform ScaleY="-1"/>

                                            </TransformGroup>
                                        </Button.RenderTransform>
                                        <Button.Background>
                                            <ImageBrush ImageSource="/Pictures/up-arrow.png" Stretch="Uniform" TileMode="None"/>
                                        </Button.Background>
                                    </Button>
                                </StackPanel>
                            </StackPanel>
                        </GroupBox>
                        <GroupBox Height="Auto" Header="Acceleration Axis">
                            <StackPanel Orientation="Horizontal">
                                <StackPanel Orientation="Vertical">
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                        <TextBlock Text="max" Width="25" Margin="5,0,5,0" FontWeight="Bold"/>
                                        <TextBox x:Name="textBoxAccMax" KeyDown="textBoxAccMax_KeyDown" AcceptsReturn="False" Width="60" HorizontalContentAlignment="Right"/>
                                        <TextBlock Text="g" Margin="5,0,0,0" FontWeight="Bold"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                        <TextBlock Text="min" Width="25" Margin="5,0,5,0" FontWeight="Bold"/>
                                        <TextBox x:Name="textBoxAccMin" KeyDown="textBoxAccMin_KeyDown" AcceptsReturn="False" Width="60" HorizontalContentAlignment="Right"/>
                                        <TextBlock Text="g" Margin="5,0,0,0" FontWeight="Bold"/>
                                    </StackPanel>
                                </StackPanel>
                                <StackPanel Orientation="Vertical" VerticalAlignment="Center" Margin="10,0,0,0">
                                    <Button x:Name="buttonLimitAccUp" Width="20" Height="15" PreviewMouseDown="buttonLimitAccUp_PreviewMouseDown" PreviewMouseUp="buttonLimitAccUp_PreviewMouseUp">
                                        <Button.Background>
                                            <ImageBrush ImageSource="/Pictures/up-arrow.png" Stretch="Uniform"/>
                                        </Button.Background>
                                    </Button>
                                    <Button x:Name="buttonLimitAccDown" Width="20" Height="15" RenderTransformOrigin="0.5,0.5" PreviewMouseDown="buttonLimitAccDown_PreviewMouseDown" PreviewMouseUp="buttonLimitAccDown_PreviewMouseUp">
                                        <Button.RenderTransform>
                                            <TransformGroup>
                                                <ScaleTransform ScaleY="-1"/>

                                            </TransformGroup>
                                        </Button.RenderTransform>
                                        <Button.Background>
                                            <ImageBrush ImageSource="/Pictures/up-arrow.png" Stretch="Uniform" TileMode="None"/>
                                        </Button.Background>
                                    </Button>
                                </StackPanel>
                            </StackPanel>
                        </GroupBox>
                        <GroupBox Height="Auto" Header="Smoothing (s)" Margin="8,0,0,0">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>

                                <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Center">
                                    <TextBlock Text="Alt" Width="45" Margin="5,0,5,0" FontWeight="Bold"/>
                                    <TextBox x:Name="textBoxSmoothAlt" Width="50" HorizontalContentAlignment="Right" KeyDown="textBoxSmoothing_KeyDown" Tag="Alt"/>
                                    <TextBlock Text="s" Margin="5,0,0,0" FontWeight="Bold"/>
                                </StackPanel>
                                <StackPanel Grid.Row="1" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Center">
                                    <TextBlock Text="Temp" Width="45" Margin="5,0,5,0" FontWeight="Bold"/>
                                    <TextBox x:Name="textBoxSmoothTemp" Width="50" HorizontalContentAlignment="Right" KeyDown="textBoxSmoothing_KeyDown" Tag="Temp"/>
                                    <TextBlock Text="s" Margin="5,0,0,0" FontWeight="Bold"/>
                                </StackPanel>
                                <StackPanel Grid.Row="2" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Center">
                                    <TextBlock Text="Acc" Width="45" Margin="5,0,5,0" FontWeight="Bold"/>
                                    <TextBox x:Name="textBoxSmoothAccAbs" Width="50" HorizontalContentAlignment="Right" KeyDown="textBoxSmoothing_KeyDown" Tag="AccAbs"/>
                                    <TextBlock Text="s" Margin="5,0,0,0" FontWeight="Bold"/>
                                </StackPanel>

                                <StackPanel Grid.Row="0" Grid.Column="1" Orientation="Horizontal" HorizontalAlignment="Center">
                                    <TextBlock Text="Acc-X" Width="45" Margin="10,0,5,0" FontWeight="Bold"/>
                                    <TextBox x:Name="textBoxSmoothAccX" Width="50" HorizontalContentAlignment="Right" KeyDown="textBoxSmoothing_KeyDown" Tag="AccX"/>
                                    <TextBlock Text="s" Margin="5,0,0,0" FontWeight="Bold"/>
                                </StackPanel>
                                <StackPanel Grid.Row="1" Grid.Column="1" Orientation="Horizontal" HorizontalAlignment="Center">
                                    <TextBlock Text="Acc-Y" Width="45" Margin="10,0,5,0" FontWeight="Bold"/>
